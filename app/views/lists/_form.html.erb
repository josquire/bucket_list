<%= form_for [@bucket, @list] do |f| %>
  <%= f.label :category %>
  <%= f.select :category, List.category %>
  <br/>
  <%= f.label :location %>
  <%= f.text_field :location %>
  <br/>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <br/>
  <%= f.check_box :completed %>
  <%= f.label :completed %>
  <br/>
  <div class='waves-effect waves-light btn'>
  <%= f.submit :submit %>
  </div>
  <% end %>
