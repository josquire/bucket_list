<div class="row">
	 <div class="col s12 m6">
		 <div class="card">
			 <div class="card-content card-action">
				 <span class="card-title"><h2>Your Buckets</h2></span>
				 <% if @buckets.any? %>
				 <ul>
					 <% @buckets.each do |bucket| %>
					 <li class="nav">
						 <h3><%= link_to bucket.name, bucket_path(bucket) %></h3>
					 </li>
					 <% end %>
				 </ul>
				 <% else %>
				 <% end %>
			 </div>
			 <div class="card-action">
				 <a><%= link_to "Add A Bucket", new_bucket_path %> </a>
			 </div>
		 </div>
	 </div>
 </div>
